<aside class="right-side">
  <!--if-->
  <section ng-show="loading_area" class="content-header">
    <center><%=image_tag("loading.gif")%></center>
  </section>
  <!--else-->
  <section ng-if='!loading_area && area.id' ng-include 
    src="'indicadores_operativos/_navigation.html'" class="content-header">
  </section>
      <!-- Main content -->
  <section ng-if='!loading_area && area.id' class="content">
    <div class='row'>
      <section class="col-lg-12">
        <div class="nav-tabs-custom" style="cursor: move;">
          <ul class="nav nav-tabs pull-right">
            <li class="pull-left header">
              <i class="fa fa-list-ol"></i> Indicadores Operativos
            </li>
            <li ng-class="{'active': state == 'indicadores'}">
              <a href ng-click='listarProcesos()' data-toggle="tab">
                Listado
              </a>
            </li>
          </ul>
        </div>
      </section>  
    </div>
    <div class="row">
      <div ng-switch on="state"> 
        <div ng-switch-when="indicadores">
          <div ng-include src="'indicadores_operativos/list.html'"></div>
        </div>
      </div>      
    </div>
  </section>
</aside>

